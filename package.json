{
  "name": "react-bs-notifier",
  "version": "1.1.0",
  "description": "A react component to show growl-like notifications using bootstrap alerts",
  "keywords": [
    "react-component",
    "react",
    "growl",
    "notifier",
    "notifications",
    "bootstrap",
    "alerts"
  ],
  "main": "alerts.js",
  "author": "Chad Lee <npm@chadly.net>",
  "license": "MIT",
  "scripts": {
    "start": "cp node_modules/bootstrap/dist/css/bootstrap.css example && webpack-dev-server --progress --inline --hot --content-base example/ --port 1341",
    "build": "babel alerts.jsx > alerts.js",
    "lint": "eslint .",
    "prepublish": "npm run build && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadly/react-bs-notifier"
  },
  "bugs": {
    "url": "https://github.com/chadly/react-bs-notifier/issues"
  },
  "browserify": {
    "transform": [
      "node-lessify"
    ]
  },
  "dependencies": {
    "jss": "^4.0.3",
    "jss-camel-case": "^2.0.0",
    "jss-default-unit": "^4.0.1",
    "jss-extend": "^2.0.1",
    "jss-nested": "^2.4.1",
    "jss-vendor-prefixer": "^3.0.0",
    "react-jss": "^3.0.1"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-addons-css-transition-group": ">=0.14"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.3.13",
    "bootstrap": "^3.3.7",
    "eslint": "^3.4.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-react": "^6.2.0",
    "json-loader": "^0.5.4",
    "json-update": "^3.0.0",
    "mocha": "^2.2.5",
    "mocha-teamcity-reporter": "1.0.x",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.15.1"
  }
}
