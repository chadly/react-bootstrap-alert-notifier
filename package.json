{
  "name": "react-bs-notifier",
  "version": "3.0.0",
  "description": "A react component to show growl-like notifications using bootstrap alerts",
  "keywords": [
    "react-component",
    "react",
    "growl",
    "toaster",
    "notifier",
    "notifications",
    "bootstrap",
    "alerts"
  ],
  "main": "lib/alerts.js",
  "author": "Chad Lee <npm@chadly.net>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --progress --inline --hot --content-base example/ --port 1341",
    "compile": "rimraf lib && babel src -d lib",
    "build": "webpack --progress --bail",
    "lint": "eslint src --ext=js,jsx",
    "prepublish": "npm run lint && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadly/react-bs-notifier"
  },
  "bugs": {
    "url": "https://github.com/chadly/react-bs-notifier/issues"
  },
  "dependencies": {
    "jss": "5.x",
    "jss-camel-case": "2.x",
    "jss-default-unit": "4.x",
    "jss-extend": "2.x",
    "jss-nested": "2.x",
    "jss-vendor-prefixer": "3.x",
    "react-jss": "3.x",
    "toetag": "3.3.x"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-addons-css-transition-group": ">=0.14"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.3.13",
    "eslint": "^3.4.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-react": "^6.2.0",
    "json-loader": "0.5.x",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.15.1"
  }
}
